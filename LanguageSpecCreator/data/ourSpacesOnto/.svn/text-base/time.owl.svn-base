          <owl:ObjectProperty rdf:about="#unitType"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#gMonth"
        >--01</owl:hasValue>
        <owl:onProperty>
          <owl:DatatypeProperty rdf:about="#month"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
