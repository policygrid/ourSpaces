package common;

public class BlogBean {
	
	public int blogID;
	public int userid;
	public String rdfUserID;
	public String container;
	public String rdfBlogID;
	public String content;
	public String title;
	public String date;
	public String about;
	public String rdfCommentID;
	
	public BlogBean()
	{
		
	}
	
	/**
	 * returns the blog's database numerical ID
	 * @return
	 */
	public int getID()
	{
		return blogID;
	}
	
	public int getUserID()
	{
		return userid;
	}
	
	public String getRdfCommentID()
	{
		return rdfCommentID;
	}
	
	public void setRdfCommentID(String rdfCommentID)
	{
		this.rdfCommentID = rdfCommentID;
	}
	
	public String getAbout()
	{
		return about;
	}
	
	public void setAbout(String about)
	{
		this.about = about;
	}
	
	public void setUserID(int userid)
	{
		this.userid = userid;
	}
	
	public String getRDFUserID()
	{
		return rdfUserID;
	}
	
	public void setRdfUserID(String rdfUserID)
	{
		this.rdfUserID = rdfUserID;
	}
	
	public String getContainer()
	{
		return container;
	}
	
	public void setContainer(String container)
	{
		this.container = container;
	}
	
	public String getRdfBlogID()
	{
		return rdfBlogID;
	}
	
	public void setRdfBlogID(String rdfBlogID)
	{
		this.rdfBlogID = rdfBlogID;
	}
	
	/**
	 * Returns the blog's content
	 * @return
	 */
	public String getContent()
	{
		return content;
	}
	
	/**
	 * Returns the blog's title
	 * @return
	 */
	public String getTitle()
	{
		return title;
	}
	
	/**
	 * Returns the blog's date
	 * @return
	 */
	public String getDate()
	{
		return date;
	}
	
	/**
	 * Sets the blog's ID
	 * @param id
	 */
	public void setID(int blogID)
	{
		this.blogID = blogID;
	}
	
	/**
	 * Sets the blog's content
	 * @param content
	 */
	public void setContent(String content)
	{
		this.content = content;
	}
	
	/**
	 * Sets the blog's title
	 * @param title
	 */
	public void setTitle(String title)
	{
		this.title = title;
	}
	
	/**
	 * Sets the blog's date.
	 * @param date
	 */
	public void setDate(String date)
	{
		this.date = date;
	}
}
