package common;

public class Box {
	String name;
	int status;
	int column;
	int position;
	
	public Box(String name, int column, int status, int position){
		this.name = name;
		this.status = status;
		this.column = column;
		this.position = position;
	}
	
	public Box(){
		
	}
	
	public String getName(){
		return name;
	}
	
	public int getStatus(){
		return status;
	}
	
	public int getColumn(){
		return column;
	}

	public int getPosition(){
		return position;
	}
	
	
}
