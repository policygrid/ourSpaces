package common;

/**
 * Handles the storing of Project Blogs in the form of a JavaBean.
 * 
 * @author Richard Reid
 * @version 1.0
 */

public class ProjectBlogBean 
{
	
	public String container;
	public String title;
	
	public ProjectBlogBean()
	{
		
	}
	
	
	/**
	 * 
	 * @return blog container
	 */
	public String getBlogContainer()
	{
		return container;
	}
	
	/**
	 * 
	 * @return blog title
	 */
	public String getTitle()
	{
		return title;
	}
	
	
	/**
	 * Set the blog's container
	 * @param content
	 */
	public void setContainer(String container)
	{
		this.container = container;
	}
	
	/**
	 * Assign a title to the blog container
	 * @param poster
	 */
	public void setTitle(String title)
	{
		this.title = title;
	}

}
